                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary">Mange Users</h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Phone Number</th>
                                            <th>Email</th>
                                            <th>Block/Unblock</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each userinfo}}
                                        <tr>
                                            <td>{{this.name}}</td>
                                            <td>{{this.phoneNumber}}</td>
                                            <td>{{this.email}}</td>
                                            <td>
                                                {{#if block}}
                                                <a href="/admin/unblockUser/{{this._id}}" class="btn btn-success"
                                                    onclick="return confirm('Are you want to Unblock {{this.name}}?')">Unblock</a>
                                                {{else}}
                                                <a href="/admin/blockUser/{{this._id}}" class="btn btn-danger"
                                                    onclick="return confirm('Are you want to block {{this.name}}?')">Block</a>
                                                {{/if}}
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

